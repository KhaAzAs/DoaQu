<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="notification_right_side_padding_top">4dp</dimen>
    <style name="MaturascTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:fontFamily">@font/maturasc</item>
        <item name="android:textColor">@color/colorPrimaryContent</item>
    </style>
    <style name="TextAppearance.AppCompat.Tooltip">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="tab_text" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Widget.ActionBar.Subtitle</item>
        <item name="android:fontFamily">@font/maturasc</item>
    </style>
</resources>